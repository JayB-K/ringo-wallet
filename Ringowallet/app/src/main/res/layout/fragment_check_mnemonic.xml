<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_white"
    android:clipChildren="false">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center_horizontal">
        <TextView
            android:id="@+id/tv_confirm_msg0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/space_28"
            android:layout_marginRight="@dimen/space_28"
            android:layout_marginTop="@dimen/space_28"
            style="@style/font_15_black1"
            android:text="@string/msg_confirm_msg01"/>

        <TextView
            android:id="@+id/tv_confirm_msg1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_18"
            android:layout_marginLeft="@dimen/space_28"
            android:layout_marginRight="@dimen/space_28"
            style="@style/font_12_gray1"
            android:lineSpacingExtra="@dimen/space_4"
            android:text="@string/msg_confirm_msg02"/>


